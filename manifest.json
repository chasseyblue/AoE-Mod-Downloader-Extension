{
  "manifest_version": 3,
  "name": "Age of Empires Mod Downloader",
  "version": "1.0",
  "description": "Download Age of Empires mods without subscribing in game.",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "cookies"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "host_permissions": [
    "https://api.ageofempires.com/*"
  ]
}